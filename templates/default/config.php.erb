<?php

unset($CFG);  // Ignore this line
global $CFG;  // This is necessary here for PHPUnit execution

$CFG = new stdClass();
$CFG->dbtype    = '<%= @db_type %>';      // 'pgsql', 'mysqli', 'mssql', 'sqlsrv' or 'oci'
$CFG->dblibrary = '<%= @db_library %>';     // 'native' only at the moment
$CFG->dbhost    = '<%= @host %>';  // eg 'localhost' or 'db.isp.com' or IP
$CFG->dbname    = '<%= @db_name %>';     // database name, eg moodle
$CFG->dbuser    = '<%= @db_user %>';   // your database username
$CFG->dbpass    = '<%= @db_pass %>';   // your database password
$CFG->prefix    = '<%= @prefix %>';       // prefix to use for all table names
$CFG->dboptions = array(
    'dbpersist' => <%= @db_persist %>,
    'dbsocket'  => <%= @db_socket %>,
    'dbport'    => '<%= @db_port %>',
);

$CFG->wwwroot   = '<%= @wwwroot %>';
$CFG->dataroot  = '<%= @dataroot %>';

$CFG->passwordsaltmain = '<%= @passwordsaltmain %>';

$CFG->directorypermissions = <%= @directorypermissions %>;
$CFG->admin = '<%= @admin %>';

require_once dirname(__FILE__) . '/lib/setup.php'; // Do not edit